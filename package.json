{
    "name": "react-starter",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev-local": "cross-env NEXT_PUBLIC_LOCAL_SERVER=true next dev",
        "build": "next build",
        "build-local": "cross-env NEXT_PUBLIC_LOCAL_SERVER=true next build",
        "build-analyze": "cross-env ANALYZE=true yarn build",
        "export": "rimraf ./out && rimraf ./.next && cross-env NODE_ENV=production next build && next export",
        "start": "next start",
        "lint": "next lint",
        "cypress-run": "cypress run --headed",
        "cypress-run:ci": "cypress run --browser chrome --config video=false",
        "cypress-open": "cypress open --browser chrome",
        "cypress-check": "rimraf ./.cypress-pending && yarn build && yarn component-test:ci && yarn e2e-test:ci && node -e \"require('./helpers/ci/CypressCompare.js').run()\"",
        "test": "yarn unit-test && yarn component-test && yarn e2e-test",
        "unit-test": "jest",
        "e2e-test": "yarn build && start-server-and-test start 3000 cypress-run",
        "e2e-test:ci": "start-server-and-test start 3000 cypress-run:ci",
        "e2e-test-open": "start-server-and-test dev 3000 cypress-open",
        "component-test": "cypress run --component",
        "component-test:ci": "cypress run --component --browser chrome --config video=false",
        "component-test-open": "cypress open --component",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "stories-check": "rimraf ./.stories-pending && storycap --serverCmd \"start-storybook --ci -p 6007\" --serverTimeout 180000 -o .stories-pending http://localhost:6007 && kill-port 6007 && node -e \"require('./helpers/ci/StoriesCompare').run()\""
    },
    "dependencies": {
        "@emotion/cache": "^11.9.3",
        "@emotion/react": "^11.9.3",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.9.3",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.8.4",
        "@mui/styles": "^5.8.4",
        "axios": "^0.27.2",
        "clsx": "^1.1.1",
        "mobx": "^6.6.0",
        "mobx-react-lite": "^3.4.0",
        "next": "^12.1.6",
        "next-react-svg": "^1.1.1",
        "noty": "^3.2.0-beta",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "sass": "^1.52.3",
        "typescript": "^4.7.3",
        "webpack": "^5.73.0"
    },
    "devDependencies": {
        "@babel/core": "^7.18.5",
        "@babel/eslint-parser": "^7.18.2",
        "@babel/plugin-proposal-decorators": "^7.18.2",
        "@babel/preset-react": "^7.17.12",
        "@cypress/react": "^6.0.0",
        "@cypress/webpack-dev-server": "^2.0.0",
        "@next/bundle-analyzer": "^12.1.6",
        "@next/eslint-plugin-next": "^12.1.6",
        "@storybook/addon-actions": "^6.5.9",
        "@storybook/addon-essentials": "^6.5.9",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/addons": "^6.5.9",
        "@storybook/builder-webpack5": "^6.5.9",
        "@storybook/manager-webpack5": "^6.5.9",
        "@storybook/react": "^6.5.9",
        "@zeit/next-source-maps": "^0.0.3",
        "babel-loader": "^8.2.5",
        "chalk": "4.1.2",
        "cross-env": "^7.0.2",
        "cypress": "^10",
        "eslint": "^8.18.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-next": "^12.1.6",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-react": "7.30.0",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-storybook": "^0.5.12",
        "fs-extra": "^10.1.0",
        "html-webpack-plugin": "5",
        "jest": "^28.1.1",
        "jest-environment-jsdom": "^28.1.1",
        "kill-port": "^2.0.0",
        "next-compose-plugins": "^2.2.0",
        "postcss": "^8.4.14",
        "puppeteer": "^14.4.0",
        "rimraf": "^3.0.2",
        "sass-loader": "^13.0.0",
        "start-server-and-test": "^1.11.0",
        "storybook-addon-next": "^1.6.6",
        "storycap": "^3.1.8",
        "webpack-dev-server": "4.9.2"
    }
}
