{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-open": "start http://localhost:3000 && pnpm dev",
    "build": "next build",
    "build-analyze": "cross-env ANALYZE=true pnpm build",
    "export": "rimraf ./out && rimraf ./.next && cross-env NODE_ENV=production next build && next export",
    "start": "next start",
    "lint": "next lint",
    "test": "pnpm test:unit && pnpm test:component && pnpm test:e2e",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "test:e2e-open": "pnpm test:e2e --ui",
    "e2e-check": "node -e \"require('./helpers/ci/ScreenshotsCompare').comparePlaywright()\"",
    "test:component": "playwright test -c playwright-ct.config.ts",
    "test:component-open": "pnpm test:component --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "stories-check": "storycap --serverCmd \"storybook dev --ci -p 6007\" --serverTimeout 180000 -o .stories-pending http://localhost:6007 && kill-port 6007 && node -e \"require('./helpers/ci/ScreenshotsCompare').compareStorybook()\"",
    "feature:remove": "node ./helpers/featureSelect.ts"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.18",
    "@mui/material": "5.14.18",
    "axios": "1.6.0",
    "clsx": "1.2.1",
    "colorette": "2.0.19",
    "mobx": "6.12.0",
    "mobx-react-lite": "3.4.3",
    "next": "14.0.3",
    "next-react-svg": "1.2.0",
    "noty": "3.2.0-beta-deprecated",
    "pixelmatch": "5.3.0",
    "pngjs": "7.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.69.7",
    "typescript": "5.0.2",
    "webpack": "5.89.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.0.3",
    "@playwright/experimental-ct-react": "1.32.2",
    "@playwright/test": "1.32.2",
    "@storybook/addon-actions": "7.5.3",
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/addons": "7.5.3",
    "@storybook/nextjs": "7.5.3",
    "@storybook/react": "7.5.3",
    "@svgr/webpack": "8.1.0",
    "@total-typescript/ts-reset": "0.5.1",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "eslint-config-next": "14.0.3",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-storybook": "0.6.15",
    "fs-extra": "11.2.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "kill-port": "2.0.1",
    "postcss": "8.4.33",
    "puppeteer": "19.11.1",
    "rimraf": "4.4.1",
    "sass-loader": "13.3.3",
    "storybook": "7.5.3",
    "storycap": "4.0.0",
    "webpack-dev-server": "4.13.1"
  }
}
